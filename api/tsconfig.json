{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}
